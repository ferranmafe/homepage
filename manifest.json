{
  "manifest_version": 3,
  "name": "Homepage",
  "version": "1.0.0",
  "description": "Homepage",
  "chrome_url_overrides": {
    "newtab": "src/tab/index.html"
  },
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["<all_urls>"]
}
